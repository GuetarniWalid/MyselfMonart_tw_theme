{% comment %}
    Renders the scripts containing the paintings data

    Accepts:
    - product: {Object}
    - ratio: {String}
{% endcomment %}


<script id="product-data">
  window.productRatio = "{{ ratio }}";
  window.productImageSRC = "{{ product.images[1] | image_url | prepend: 'https:' }}";
  window.productImageAlt = "{{ product.images[1].alt | escape }}";
  window.canvasTexture = "{{ 'tw.main_product.painting.canvas_texture' | t }}";
  window.productId = "{{ product.id }}";
  window.productAll = {{ product | json }};
  window.shopUrl = window.Shopify.routes.root
</script>

