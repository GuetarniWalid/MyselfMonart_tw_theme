          "default": "t:sections.main-product.blocks.price.settings.third_key_benefit.default"
        },
        {
          "type": "text",
          "id": "fourth_key_benefit",
          "label": "t:sections.main-product.blocks.price.settings.fourth_key_benefit.label",
          "default": "t:sections.main-product.blocks.price.settings.fourth_key_benefit.default"
        }
      ]
    },
    {
      "type": "variant_picker",
      "name": "t:sections.main-product.blocks.variant_picker.name",
      "limit": 1
    },
    {
      "type": "description",
      "name": "t:sections.main-product.blocks.description.name",
      "limit": 1
    },
    {
      "type": "collapsible_tab",
      "name": "t:sections.main-product.blocks.collapsible_tab.name",
      "settings": [
        {
          "type": "text",
