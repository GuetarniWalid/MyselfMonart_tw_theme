<script src="{{ 'main-product.js' | asset_url }}" defer="defer"></script>
 
{% liquid
  assign first_3d_model = product.media | where: "media_type", "model" | first | default: product.metafields.files["3d_file_name"].value
  if first_3d_model != blank
    if first_3d_model.url != blank
      assign 3d_model_url = first_3d_model.url
      assign 3d_model_poster = product.media[1]
      assign 3d_model_alt = first_3d_model.alt
    else
      assign 3d_model_url = first_3d_model.sources[0].url
      assign 3d_model_poster = first_3d_model.preview_image
      assign 3d_model_alt = first_3d_model.alt
    endif 
  endif
%}
{% if first_3d_model != blank %}
  <script type="module" src="https://ajax.googleapis.com/ajax/libs/model-viewer/3.3.0/model-viewer.min.js" defer></script>
{% endif %}

{% if template.suffix == 'painting' or template.suffix == 'personalized' %}
  {% liquid
    if template.suffix == 'painting'
      if product.images[1].aspect_ratio < 1
        assign ratio = 'portrait'
        assign price_min = settings.portrait_lowest_price
      elsif product.images[1].aspect_ratio > 1
        assign ratio = 'landscape'
        assign price_min = settings.landscape_lowest_price
      else
        assign ratio = 'square'
        assign price_min = settings.square_lowest_price     
      endif
    else if template.suffix == 'personalized'
      if product.images[1].aspect_ratio < 1
        assign ratio = 'personalized portrait'
        assign price_min = settings.personalized_portrait_lowest_price
      endif
    endif
  %}
{% endif %}

<a class="px-4 py-2 opacity-0 focus-visible:opacity-100 absolute top-6 left-10 z-10 bg-secondary transition-colors duration-500" href="#ProductInfo-{{ section.id }}">
  {{ "tw.main_product.accessibility.skip_to_product_info" | t }}
</a>

<div class="md:flex md:justify-center md:gap-12">
  {% if product.template_suffix == "tapestry" %}
    <div id="croppr-top-container" class="flex-grow">
      <div class="md:sticky md:top-[120px] max-w-[550px] mx-auto relative">
        <img
          srcset="{%- if product.media[1].preview_image.width >= 400 -%}{{ product.media[1].preview_image | image_url: width: 400 }} 400w,{%- endif -%}
                  {%- if product.media[1].preview_image.width >= 550 -%}{{ product.media[1].preview_image | image_url: width: 550 }} 550w,{%- endif -%}
                  {{ product.media[1].preview_image | image_url }} {{ product.media[1].preview_image.width }}w"
          src="{{ product.media[1].preview_image | image_url: width: 550 }}"
          sizes="(min-width: 548px) 550px, 100vw"
          alt="{{ product.media[1].alt | escape }}"
          {% unless loop.first %}
            loading="lazy"
          {% endunless %}
          width="550"
          height="auto"
          data-media-id="{{ product.media[1].id }}"
            id="croppr"
        >
        {% render 'finger-touch' %}
      </div>
    </div>
  {% else %}
    <main-product-carousel>
      <div class="md:sticky md:top-[120px] relative max-w-[550px] mx-auto">
        <div class="carousel flex justify-start items-center transition-transform duration-300 mx-auto my-4 snap-x snap-mandatory overflow-y-hidden overflow-x-auto scrollbar-hidden h-[55vh] xs:h-[65vh] md:h-[55vh] lg:h-[70vh]">
          {% liquid
            for media in product.media
              if media.media_type == 'image' and forloop.index != 3
                assign nbOfMedia = nbOfMedia | default: 0 | plus: 1
                render 'tw-main-product-image', media: media, loop: forloop
              elsif forloop.index == 3 and first_3d_model != blank
                if first_3d_model.url != blank
                  assign nbOfMedia = nbOfMedia | default: 0 | plus: 2
                  render 'tw-main-product-model-3d', url: 3d_model_url, alt: 3d_model_alt, poster: 3d_model_poster
                  render 'tw-main-product-image', media: media, loop: forloop
                else
                  assign nbOfMedia = nbOfMedia | default: 0 | plus: 1
                  render 'tw-main-product-model-3d', url: 3d_model_url, alt: 3d_model_alt, poster: 3d_model_poster
                endif
              else if media.media_type == 'image'
                assign nbOfMedia = nbOfMedia | default: 0 | plus: 1
                render 'tw-main-product-image', media: media, loop: forloop
              endif
            endfor
          %}
        </div>
        {% if product.media.size > 1 %}
          <div class="absolute bg-secondary transition-colors duration-500 rounded-full left-1/2 -translate-x-1/2 bottom-3 border-main border-1 md:hidden flex justify-center items-center h-[47px] overflow-hidden">
            <button class="previous pl-5 pr-3 h-full focus-visible:outline-none focus-visible:bg-main-10 hover:bg-main-10">
              <div class="rotate-90">{% render 'tw-icon-caret', width: '18' %}</div>
            </button>
            <span class="px-3"><span class="image-counter">1</span>/{{ nbOfMedia }}</span>
            <button class="next pr-5 pl-3 h-full focus-visible:outline-none focus-visible:bg-main-10 hover:bg-main-10">
              <div class="-rotate-90">{% render 'tw-icon-caret', width: '18' %}</div>
            </button>
          </div>
        {% endif %}
        <div class="hidden md:flex flex-wrap gap-2">
          {% liquid
              for media in product.media
                if media.media_type == 'image' and forloop.index != 3
                  assign image_number = image_number | default: 0 | plus: 1
                  render 'tw-main-product-thumbnail-image', image: media.preview_image, alt: media.alt, object-fit: 'cover', image_number: image_number
                elsif forloop.index == 3 and first_3d_model != blank
                  if first_3d_model.url != blank
                    assign image_number = image_number | default: 0 | plus: 1
                    render 'tw-main-product-thumbnail-image', image: product.media[1]preview_image, object-fit: 'contain', alt: 3d_model_alt, file: 'glb', image_number: image_number
                    assign image_number = image_number | default: 0 | plus: 1
                    render 'tw-main-product-thumbnail-image', image: media.preview_image, alt: media.alt, object-fit: 'cover', image_number: image_number
                  else
                    assign image_number = image_number | default: 0 | plus: 1
                    render 'tw-main-product-thumbnail-image', image: media.preview_image, alt: media.alt, object-fit: 'cover', file: 'glb', image_number: image_number
                  endif
                else if media.media_type == 'image'
                  assign image_number = image_number | default: 0 | plus: 1
                  render 'tw-main-product-thumbnail-image', image: media.preview_image, alt: media.alt, object-fit: 'cover', image_number: image_number
                endif
              endfor
            %}
        </div>
      </div>

      <div class="popup fixed top-0 left-0 w-full h-dvh overflow-auto scrollbar bg-secondary transition-colors duration-500 z-50 hidden p-0 focus:outline-none" role="dialog" aria-modal="false" aria-label="{{ 'tw.main_product.carousel.popup.title' | t }}" tabindex="0">
        <button class="absolute z-10 top-3 right-3 bg-secondary-50 transition-colors duration-500 rounded-full p-2 focus-visible:outline-main" type="button" aria-label="{{ 'tw.main_product.carousel.popup.close' | t }}" tabindex="-1">
        {% render 'tw-icon-close', width: '22' %}
        </button>
        <template id="main-product-popup">
        {%- for media in product.media -%}
          <popup-image>
            {%- render 'tw-main-product-popup-image', media: media, loop: forloop -%}
          </popup-image>
        {%- endfor -%}
        </template>
      </div>
    </main-product-carousel>
  {% endif %}
  <main-product-blocks class="grow" id="ProductInfo-{{ section.id }}" data-layout="{{ template.suffix }}">
    <div class="max-w-lg px-4 sm:px-0 mx-auto my-4">
      {%- for block in section.blocks -%}
        {%- case block.type -%}
          {%- when '@app' -%}
            {% render block %}
          {%- when 'text' -%}
            <div class="flex flex-col gap-4 mb-4 text-lg">{{- block.settings.text -}}</div>
          {%- when 'title' -%}
            <h1 class="text-4xl {{ block.settings.margin_bottom }}">{{ product.title | escape }}</h1>
          {%- when 'price' -%}
            {% if block.settings.style == 'default' %}
              <span id="main-price" class="block text-xl font-bold font-heading mb-4" data-money-symbol="{{ cart.currency.symbol }}">{{ price_min | money_with_currency }}</span>
            {% elsif block.settings.style == 'tapestry' %}
              <div class="flex gap-4 mb-4 py-6 rounded-2xl px-7 min-h-20 mt-5" style="background: {{ block.settings.background }}" data-money-symbol="{{ cart.currency.symbol }}">
                <div id="main-price" class=" flex items-center text-5xl font-bold font-heading border-r-2 border-black/5 pr-7">{{ price_min | money_with_currency }}</div>
                <div class="flex items-center overflow-hidden">
                  <ul class="max-w-full">
                    <li class="flex items-center mb-2 overflow-hidden text-ellipsis whitespace-nowrap">{%  render 'icon-check' %}&nbsp;&nbsp;<span class="overflow-hidden text-ellipsis whitespace-nowrap"><span id="nb-tapestry-strips">3</span> l√©s</span></li>
                    {% if block.settings.second_key_benefit != blank %}
                      <li class="flex items-center mb-2 overflow-hidden text-ellipsis whitespace-nowrap max-w-full">{%  render 'icon-check' %}&nbsp;&nbsp;<span class="overflow-hidden text-ellipsis whitespace-nowrap">{{ block.settings.second_key_benefit }}</span></li>
                    {% endif %}
                    {% if block.settings.third_key_benefit != blank %}
                      <li class="flex items-center mb-2">{%  render 'icon-check' %}&nbsp;&nbsp;<span class="overflow-hidden text-ellipsis whitespace-nowrap">{{ block.settings.third_key_benefit }}</span></li>
                    {% endif %}
                    {% if block.settings.fourth_key_benefit != blank %}
                      <li class="flex items-center overflow-hidden text-ellipsis whitespace-nowrap">{%  render 'icon-check' %}&nbsp;&nbsp;<span class="overflow-hidden text-ellipsis whitespace-nowrap">{{ block.settings.fourth_key_benefit }}</span></li>
                    {% endif %}
                  </ul>
                </div>
              </div>
            {% endif %}
          {%- when 'variant_picker' -%}
            {% render 'product-variant-picker', product: product, block: block, product_form_id: product_form_id %}
          {%- when 'buy' -%}
            {% style %}
              :root {
                --color-action: {{ block.settings.main_color.red }}, {{ block.settings.main_color.green }}, {{ block.settings.main_color.blue }};
                --color-action-hover: {{ block.settings.hover_color }};
              }
            {% endstyle %}
